{
  "compilerOptions": {
    // "types": ["node"],
    // "noEmit": false, // enable to genrate compiled js file in outDir
    "target": "ES6",
    "module": "commonjs",
    // "strict": true,
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": false,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": "./"
  },

  "include": ["src/**/*.ts", "index.ts"]
}
