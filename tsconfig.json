{
  "compilerOptions": {
    // "types": ["node", "dotenv"],
    "noEmit": true, // enable to genrate compiled js file in outDir
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    // "strict": true,
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": false,
    "noImplicitAny": false,
    "strictNullChecks": true
  },
  "include": ["src/**/*.ts"]
}
