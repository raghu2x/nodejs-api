{
  "compilerOptions": {
    "noEmit": true,
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    // "strict": true,
    "esModuleInterop": true,
    "allowJs": true,
    "checkJs": true,
    "noImplicitAny": false,
    "strictNullChecks": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["**/*.js"]
}
